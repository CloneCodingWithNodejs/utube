extends layouts/main
include mixins/videoBlock

block content 
    .search__header
        h3 Searhching By #{searchingBy}
    .search__videos
         if videos.length === 0
            h5 No Videos Found
         each potato in videos
            +videoBlock({
                title : potato.title,
                views : potato.views,
                fileUrl : potato.fileUrl,
                id : potato.id
            })